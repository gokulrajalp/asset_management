          hr.dropdown-divider
        li
          a.dropdown-item(href='#!') Logout
#layoutSidenav
  #layoutSidenav_nav
    nav#sidenavAccordion.sb-sidenav.accordion.sb-sidenav-dark
      .sb-sidenav-menu
        .nav
          .sb-sidenav-menu-heading Core
          a.nav-link(href='/dashboard')
            .sb-nav-link-icon
              i.fas.fa-tachometer-alt
            |                                 Dashboard
          .sb-sidenav-menu-heading Interface
          a.nav-link.collapsed(href='#' data-bs-toggle='collapse' data-bs-target='#collapseLayouts-Employee-master' aria-expanded='false' aria-controls='collapseLayouts')
            .sb-nav-link-icon
              i.fas.fa-columns
            |                                 Employee Master
            .sb-sidenav-collapse-arrow
              i.fas.fa-angle-down
          #collapseLayouts-Employee-master.collapse(aria-labelledby='headingOne' data-bs-parent='#sidenavAccordion')
            nav.sb-sidenav-menu-nested.nav
              a.nav-link(href='/employee_master') All Employee
              a.nav-link(href='#addEmployeeModal', data-bs-toggle='modal') Add Employee
          a.nav-link.collapsed(href='#' data-bs-toggle='collapse' data-bs-target='#collapseLayouts-Asset-Category-Master' aria-expanded='false' aria-controls='collapseLayouts')
            .sb-nav-link-icon
              i.fas.fa-columns
            |                                 Asset Category Master
            .sb-sidenav-collapse-arrow
              i.fas.fa-angle-down
          #collapseLayouts-Asset-Category-Master.collapse(aria-labelledby='headingOne' data-bs-parent='#sidenavAccordion')
            nav.sb-sidenav-menu-nested.nav
              a.nav-link(href='/asset_category_master') All Category
              a.nav-link(href='#addAssetCategoryModal', data-bs-toggle='modal') Add Category
          a.nav-link.collapsed(href='#' data-bs-toggle='collapse' data-bs-target='#collapseLayouts-Asset-Master' aria-expanded='false' aria-controls='collapseLayouts')
            .sb-nav-link-icon
              i.fas.fa-columns
            |                                 Asset Master
            .sb-sidenav-collapse-arrow
              i.fas.fa-angle-down
          #collapseLayouts-Asset-Master.collapse(aria-labelledby='headingOne' data-bs-parent='#sidenavAccordion')
            nav.sb-sidenav-menu-nested.nav
              a.nav-link(href='/asset_master') All Asset
              a.nav-link(href='layout-static.html') Add Asset
      .sb-sidenav-footer
        .small Logged in as:
        |                         Admin
  #layoutSidenav_content
    main
      block content